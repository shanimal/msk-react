{
  "name": "msk",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "build": "npm run build-css && react-scripts build",
    "build-css": "for file in `find ./src -name '*.less'`; do ./node_modules/less/bin/lessc --verbose ${file} ${file%.less}.css; done",
    "start": "npm run build-css && npm run start-js",
    "start-js": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "devDependencies": {
    "less": "^3.0.1",
    "react-router-dom": "^4.2.2"
  }
}
